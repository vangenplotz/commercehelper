{
    "name": "vangenplotz/vp-commerce-helper",
    "description": "Auto set price to 0 and stock to unlimited for products if not set when saving product.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "vpcommercehelper"
    ],
    "support": {
        "docs": "https://github.com/https://github.com/vangenplotz/commercehelper/vp-commerce-helper/blob/master/README.md",
        "issues": "https://github.com/https://github.com/vangenplotz/commercehelper/vp-commerce-helper/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stian Dalen",
            "homepage": "https://vangenplotz.no"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "vangenplotz\\vpcommercehelper\\": "src/"
        }
    },
    "extra": {
        "name": "vpCommerceHelper",
        "handle": "vp-commerce-helper",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/https://github.com/vangenplotz/commercehelper/vp-commerce-helper/master/CHANGELOG.md",
        "class": "vangenplotz\\vpcommercehelper\\VpCommerceHelper"
    }
}
